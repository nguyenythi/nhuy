<extentreports>
    <configuration>
        <!--  standard, dark  -->
        <theme>dark</theme>
        <!--  document encoding  -->
        <!--  defaults to UTF-8  -->
        <encoding>UTF-8</encoding>
        <!--  protocol for script and stylesheets  -->
        <!--  defaults to https  -->
        <protocol>http</protocol>
        <!--  title of the document  -->
        <documentTitle>Report Test</documentTitle>
        <!--  displayed at top-nav  -->
        <reportName>Execution Result</reportName>
        <!--  timestamp format  -->
        <timeStampFormat>dd/MM/yyyy HH:mm:ss</timeStampFormat>
        <!--  custom javascript  -->
        <scripts>
            <![CDATA[

 $(document).ready(function() {

 });
]]>
        </scripts>
        <!--  custom styles  -->
        <styles>
            <![CDATA[

 ]]>
        </styles>
    </configuration>
</extentreports>